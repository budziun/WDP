#include <iostream>
#include <vector>
using namespace std;

void wypisz(vector<int>tab) {
    for (int i = 0; i < tab.size(); i++) {
        cout << tab[i] << ".";
    }
}
int max(vector<int>tab) {
    int maks = 0;
    for (int i = 0; i < tab.size(); i++) {
        if (tab[i] > maks) {
            maks = tab[i];
        }
    }
    return maks;
}
bool sprawdzam(vector<int>tab,int element) {
    for (int i = 0; i < tab.size(); i++) {
        if (element == tab[i]) {
            return true;
        }
    }
    return false;
}
bool czynalezy(vector<int>tab1, vector<int>tab2) {
    for (int i = 0; i < tab2.size(); i++) {
        if (!sprawdzam(tab1, tab2[i])){
            return false;
        }
    }
    return true;
}
vector<int> unikalne(vector<int>tab) {
    vector<int>wynik;
    for (int i = 0; i < tab.size(); i++) {
        if (!czynalezy(wynik,tab)) {
            wynik.push_back(tab[i]);
        }
    }
    return wynik;
}
int main()
{
    vector<int>tab{ 2,1,3,7,2 };
    int element = 3;
    wypisz(tab);
    cout << endl << max(tab);
    cout << endl << sprawdzam(tab, element);
    vector<int>tab1{ 2,1,3,7,8,9 };
    vector<int>tab2{ 2,1,3,7 };
    cout << endl << unikalne(tab);
    
    
}

//Kilka punktow
// zad. nalezy ze zbioru liczb wybrac liczby ktore wystepuja unikalnie
// 1. wymyslic przyklady
// 2. sprawdz czy przyklad jest kompletny
// 3. sformalizowanie parametru
// 4. naglowek funkcji
// 5. cialo funkcji
// 6. nie wiesz co dalej pisac? to wróć do przykładu
